<div id="home">
  <div class="row">
    <img src="/img/logo.png" alt="" class="mx-auto d-block">
  </div>
  <div class="row">
    <!-- Search form -->
    <input class="form-control col-8 mx-auto" type="text" placeholder="Search codeBucket" aria-label="Search">
  </div>
  <div class="row">
    <h3>Topics</h3>
    <ul id="topic-list" class="list-group">
      {{#each Topics}}
      <li data-id={{this.id}} class="list-group-item">
        <a href="topic/{{this.id}}">{{this.topicName}}</a>
        <button class="btn btn-danger float-right delete">ï½˜</button>
      </li>
      {{/each}}
    </ul>
  </div>


</div>