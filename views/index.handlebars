<div id="home">
  <div class="row">
    <img src="/img/logo.png" alt="" class="mx-auto d-block">
  </div>
  <div class="row">
    <!-- Search form -->
    <input class="form-control col-8 mx-auto" type="text" placeholder="Search codeBucket" aria-label="Search">
  </div>
  <h3>Topics</h3>
  <div class="row justify-content-around">
    {{#each topic}}
    <a href="topic/{{this.id}}">
      <button type="button" class="btn btn-lg btn-deep-purple waves-effect">
        {{this.topicName}}
      </button>
    </a>
    {{/each}}
  </div>


</div>